{
  "name": "timbercode.gitlab.io",
  "version": "2.0.0",
  "license": "MIT",
  "description": "Website of Timbercode company",
  "private": true,
  "main": "app/timbercode.pl.js",
  "files": [
    "app/",
    "lambda.js",
    "yarn.lock"
  ],
  "scripts": {
    "start": "ntl --info",
    "serve": "node app/server.js",
    "test": "eslint ./",
    "configure": "npm run configure-proxy-wrapper && npm run configure-aws",
    "configure-proxy-wrapper": "claudia generate-serverless-express-proxy --express-module app/lambda-express-app --proxy-module-name lambda",
    "configure-aws": "claudia create --region ${AWS_REGION} --name timbercode-website --handler lambda.handler --deploy-proxy-api --version initial",
    "deploy-testing": "claudia update --version testing",
    "deploy-production": "claudia update --version production"
  },
  "dependencies": {
    "aws-serverless-express": "2.2.0",
    "express": "4.15.2"
  },
  "devDependencies": {
    "claudia": "2.10.0",
    "eslint": "3.19.0",
    "eslint-config-standard": "10.2.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-node": "4.2.2",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "ntl": "1.2.0"
  },
  "engines": {
    "node": "6.10.1"
  },
  "author": {
    "name": "Pawe≈Ç Barszcz",
    "email": "pawelbarszcz@gmail.com",
    "url": "http://timbercode.pl"
  },
  "homepage": "https://gitlab.com/timbercode/timbercode.gitlab.io#README",
  "bugs": {
    "url": "https://gitlab.com/timbercode/timbercode.gitlab.io/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/timbercode/timbercode.gitlab.io.git"
  },
  "keywords": [
    "Timbercode",
    "IT",
    "quality",
    "aesthetics",
    "clean code"
  ]
}
